{
  "name": "aladdin-electron",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/start.js",
  "dependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "aws-sdk": "^2.573.0",
    "electron": "^18.3.7",
    "electron-is-dev": "^1.1.0",
    "firebase": "^7.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "electron": "electron .",
    "start": " cross-env BROWSER=none npm-run-all -p build:style watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:style build-js",
    "build:style":"tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "watch:css": "postcss src/styles/main.css -o src/index.css -w",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "aladdin.electron.0.1",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "category": "photo",
      "iconUrl": "path-to-icon.png"
    },
    "mac": {
      "category": "photo",
      "iconUrl": "path-to-icon.png"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.5.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-nested": "^4.2.1",
    "react-testing-library": "^8.0.1",
    "tailwindcss": "^1.1.3"
  }
}
